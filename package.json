{
  "name": "aserradero-landing",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "concurrently -k \"npm:dev:server\" \"vite\"",
    "dev:server": "ts-node-dev --respawn --transpile-only server/contact.ts",
    "build": "node scripts/generate-sitemap.mjs && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.vue",
    "format": "prettier --write .",
    "test": "vitest run"
  },
  "dependencies": {
    "concurrently": "^9.0.0",
    "node-fetch": "^3.3.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.0",
    "vue": "^3.5.0",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@types/node": "^22.5.0",
    "@vercel/node": "^5.3.13",
    "@vitejs/plugin-vue": "^6.0.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-plugin-vue": "^9.27.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.6.2",
    "vite": "^7.1.3",
    "vitest": "^2.1.1"
  }
}